{
    "info": {
        "_postman_id": "8b8b8b8b-8b8b-8b8b-8b8b-8b8b8b8b8b8b",
        "name": "Traccar Authentication",
        "description": "Postman collection for Traccar authentication API (Login and Signup).",
        "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
    },
    "item": [
        {
            "name": "Signup",
            "request": {
                "method": "POST",
                "header": [
                    {
                        "key": "Content-Type",
                        "value": "application/json"
                    }
                ],
                "body": {
                    "mode": "raw",
                    "raw": "{\n    \"name\": \"New User\",\n    \"email\": \"user@example.com\",\n    \"password\": \"password123\"\n}"
                },
                "url": {
                    "raw": "{{baseUrl}}/users",
                    "host": [
                        "{{baseUrl}}"
                    ],
                    "path": [
                        "users"
                    ]
                }
            },
            "response": []
        },
        {
            "name": "Login",
            "request": {
                "method": "POST",
                "header": [],
                "body": {
                    "mode": "urlencoded",
                    "urlencoded": [
                        {
                            "key": "email",
                            "value": "user@example.com",
                            "type": "text"
                        },
                        {
                            "key": "password",
                            "value": "password123",
                            "type": "text"
                        }
                    ]
                },
                "url": {
                    "raw": "{{baseUrl}}/session",
                    "host": [
                        "{{baseUrl}}"
                    ],
                    "path": [
                        "session"
                    ]
                }
            },
            "response": []
        }
    ],
    "variable": [
        {
            "key": "baseUrl",
            "value": "http://localhost:8082/api",
            "type": "string"
        }
    ]
}